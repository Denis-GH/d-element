@font-face {
  font-family: Inter;
  src:
    local("Inter-Regular"),
    local("Inter Regular"),
    url("#assets/fonts/Inter-Regular.woff2") format("woff2");
  font-weight: 400;
  font-style: normal;
  font-display: swap;
}

@font-face {
  font-family: Inter;
  src:
    local("Inter-Bold"),
    local("Inter Bold"),
    url("#assets/fonts/Inter-Bold.woff2") format("woff2");
  font-weight: 700;
  font-style: normal;
  font-display: swap;
}
